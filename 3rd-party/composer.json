{
    "require": {
        "erusev/parsedown": "@stable",
        "wpackagist-plugin/advanced-custom-fields":"^5.9.5"
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
